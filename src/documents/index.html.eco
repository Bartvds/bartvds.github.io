---
layout: 'content'
title: 'Home'
---

<div class="ui row">
	<h2 class="header">
		Home
	</h2>

	<p>Work &amp; projects for Bart van der Schoor</p>

	<div class="ui row">
		<ul class="ui list">
			<% for item in @getCollection('projects').sortArray(date: -1): %>
			<a class="item" href="<%= item.url %>">
				<i class="right triangle icon"></i> <%- item.title or item.name %></a>
			<%- item.description %>
			<% end %>
		</ul>
	</div>
</div>
